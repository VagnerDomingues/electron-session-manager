# ChatGPT Runtime Configuration
APP_NAME=ChatGPT Sessions
APP_DESCRIPTION=ChatGPT - AI Assistant
APP_URL=https://chatgpt.com
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Safari/537.36

# Session Management Configuration
ENABLE_MULTIPLE_SESSION_IDS=true

# Window Configuration
WINDOW_WIDTH=1200
WINDOW_HEIGHT=800
WINDOW_MIN_WIDTH=800
WINDOW_MIN_HEIGHT=600
WINDOW_RESIZABLE=true
WINDOW_MAXIMIZABLE=true
WINDOW_MINIMIZABLE=true

# Icons
WIN_ICON=assets/Appicons/ChatGPT.ico
MAC_ICON=assets/Appicons/ChatGPT.png

# Security Configuration
NODE_INTEGRATION=false
CONTEXT_ISOLATION=true
ENABLE_REMOTE_MODULE=false
WEB_SECURITY=true
